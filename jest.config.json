{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "setupFiles": ["dotenv/config"],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/constants.ts",
      "!src/helpers.ts",
      "!src/routes/routes.ts",
      "!src/utils.ts",
      "!src/data/**",
      "!src/cluster/**",
      "!src/app.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }